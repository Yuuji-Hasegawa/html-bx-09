{
  "name": "html-boiler-2023",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && prettier --write dist/**/*.html",
    "preview": "vite preview",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"src/**/*.js\"",
    "lint:prettier": "prettier --check src",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "prettier --write src",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "image": "run-s image:clean image:opt image:gen",
    "image:opt": "node npm-scripts/opt.mjs",
    "image:gen": "node npm-scripts/gen.mjs",
    "image:clean": "rimraf src/public/img",
    "icon": "run-s icon:clean icon:gen",
    "icon:gen": "node npm-scripts/icon.mjs",
    "icon:clean": "rimraf src/public/icons/icon-*.png"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "css-declaration-sorter": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "fs": "0.0.1-security",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "lint-staged": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.29",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sass": "^1.67.0",
    "sharp": "^0.33.0",
    "stylelint": "^16.0.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "terser": "^5.19.4",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "html-boiler-2023": "link:"
  }
}
